public class Automate
{

public Static void main(String[] args)
{

// Creating a new instance of the Chrome Driver
Webdriver driver = new ChromeDriver();

// Fetching the URL to open
driver.get("https://greenflag-qa-experience.dev.mycro.app")

// Implicit wait for the page to load
driver.manage().timeouts().implicitlyWait(10, TimeUnit.SECONDS);

// Entering valid Credentials for Log In
driver.findElement(By.id("Username")).sendKeys("qa@greenflag.com");
driver.findElement(By.id("Password")).sendKeys("Password1");

driver.manage().timeouts().implicitlyWait(10, TimeUnit.SECONDS);

// Clicking LogIn
driver.findElement(By.id("LogIn")).click();
driver.close();
}
}
